{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection" : "Server=myServer;User ID=myId;Password=myPassword;Database=myDatabase"
  },
  "JwtOptions" :{
    "Issuer": "myIssuer",
    "Audience": "myAudience",
    "SecretKey": "mySecretKey",
    "ExpiresHours": "myExpiresHours"
  }
}
